nav ul li a
  color #ffa500

#mobile-demo li a
  color #ffa500

nav img
  margin-top 3px

#last-nav-item
  margin-right 10px

.fa-bars
  color #ffa500

#logo
  height 70px
  width 250px
  margin-left 15px

.dropdown-content li a
  color #ffa500

.parallax-container
  height 250px

#fact-list
  list-style-type: disc
  list-style-position: inside
  margin-left 3%

  li
    list-style-type: disc
    list-style-position: inside

section
  color #616161
  margin-top 3em
  margin-bottom 3em

  li
    margin 2px 0

.gallery-container
  width 90%
  margin-left auto
  margin-right auto

.gallery-item-container
  display inline-block
  text-align: center
  color: #616161

.gallery-item
  height 230px
  width 270px

.gal-title
  margin-top: 3em


@media (min-width: 500px)
  .gallery-item
    margin-left 3em

@media (max-width: 350px)
  .gallery-item
    margin-left 0.8em

@media (max-width: 499px)
  .gallery-item
    margin-left: 4em

@media all and (min-width: 1300px) and (max-width: 2000px)
    .first
      margin-left 12%

@media all and (min-width: 1200px) and (max-width: 1299px)
    .first
      margin-left 5%

@media all and (min-width: 850px) and (max-width: 1199px)
    .first
      margin-left 0%

    .second
      margin-left: 13%

@media all and (min-width: 550px) and (max-width: 849px)
  .third
    margin-left: 22%

@media all and (min-width: 500px) and (max-width: 549px)
  .third
    margin-left: 10%

@media (max-width: 499px)
  #logo
    height 60px
